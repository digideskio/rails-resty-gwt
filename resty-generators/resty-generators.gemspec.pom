n.java,lib/generators/resty/setup/templates/empty.css~,lib/generators/resty/scaffold/scaffold_generator.rb,lib/generators/resty/templates/PlaceTokenizer.java,lib/generators/resty/templates/Cache.java~,lib/generators/resty/templates/ViewImpl.java~,lib/generators/resty/templates/EventHandler.java~,lib/generators/resty/templates/Model.java,lib/generators/resty/templates/Place.java~,lib/generators/resty/templates/View.java,lib/generators/resty/templates/Editor.java,lib/generators/resty/templates/View.ui.xml,lib/generators/resty/templates/ColumnDefintionsImpl.java~,lib/generators/resty/templates/Event.java,lib/generators/resty/templates/Event.java~,lib/generators/resty/templates/EventHandler.java,lib/generators/resty/templates/ActivityPlace.java~,lib/generators/resty/templates/Controller.java~,lib/generators/resty/templates/RestService.java,lib/generators/resty/templates/Cache.java,lib/generators/resty/templates/Activity.java,lib/generators/resty/templates/RestService.java~,lib/generators/resty/templates/Editor.ui.xml~,lib/generators/resty/templates/ViewImpl.java,lib/generators/resty/templates/Editor.ui.xml,lib/generators/resty/templates/ColumnDefinitionsImpl.java~,lib/generators/resty/templates/Place.java,lib/generators/resty/templates/View.ui.xml~,lib/generators/resty/templates/Model.java~,lib/generators/resty/templates/PlaceTokenizer.java~,lib/generators/resty/templates/View.java~,lib/generators/resty/templates/Editor.java~,lib/generators/resty/templates/Activity.java~,lib/ixtlan/core,lib/ixtlan/core/heartbeat.rb~,features/step_definitions/simple_steps.rb,features/generators.feature</files>
          <testFiles>features/generators.feature,features/step_definitions/simple_steps.rb</testFiles>
        </configuration>
      </plugin>
      <plugin>
        <groupId>de.saumya.mojo</groupId>
        <artifactId>cucumber-maven-plugin</artifactId>
        <version>${jruby.plugins.version}</version>
        <executions>
          <execution>
            <goals>
              <goal>test</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
</project>
